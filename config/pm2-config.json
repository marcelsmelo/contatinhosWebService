{
    "apps": [{
        "name": "Contatinhos-WebService",
        "script": "bin/www",
        "watch": true,
        "env": {
            "NODE_ENV": "development",
            "PORT": 80
        },
        "env_production": {
            "NODE_ENV": "production"
